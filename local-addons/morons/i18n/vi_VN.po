#. module: morons
#: model:mail.template,body_html:morons.email_template_po_assignment
msgid ""
"<div style=\"margin: 0px; padding: 0px; font-size: 13px;\">\n"
"                <p>Dear <t t-out=\"object.contributor_id.name or ''\"></t>,</p>\n"
"                <br>\n"
"                <p>You are assigned to a new task with the following details:</p>\n"
"                <br>\n"
"                <p>Deadline: <t t-out=\"object.date_deadline or ''\"></t></p>\n"
"                <p>Work amount: <t t-out=\"object.volume or ''\"></t> <t t-out=\"object.work_unit or ''\"></t></p>\n"
"                <p>Rate: <t t-out=\"object.rate or ''\"></t> <t t-out=\"object.currency_id.name or ''\"></t></p>\n"
"                <p>Total PO: <t t-out=\"object.po_value or ''\"></t> <t t-out=\"object.currency_id.name or ''\"></t></p>\n"
"                <br>\n"
"                <div style=\"margin: 16px 0px 16px 0px; text-align: center;\">\n"
"                    <a t-att-href=\"'/task/accept/%s' % object.id\" style=\"background-color: #875A7B; padding: 8px 16px 8px 16px; text-decoration: none; color: #fff; border-radius: 5px; font-size:13px;\">\n"
"                        Accept\n"
"                    </a>\n"
"                    <a t-att-href=\"'/task/decline/%s' % object.id\" style=\"background-color: #808080; padding: 8px 16px 8px 16px; text-decoration: none; color: #fff; border-radius: 5px; font-size:13px; margin-left: 10px;\">\n"
"                        Decline\n"
"                    </a>\n"
"                </div>\n"
"                <br>\n"
"                <p>If you have any questions, please contact us at <a t-att-href=\"'mailto:support@merctrans.com'\" style=\"color: #875A7B;\">support@merctrans.com</a></p>\n"
"                <br>\n"
"            </div>\n"
"        "
msgstr ""

#. module: morons
#: model:mail.template,body_html:morons.email_template_invoice_paid
msgid ""
"<div style=\"margin: 0px; padding: 0px; font-size: 13px;\">\n"
"                <p>Dear <t t-out=\"object.partner_id.name or 'Contributor'\"></t>,</p>\n"
"                <br>\n"
"                <p>Your submitted invoice <t t-out=\"object.name or ''\"></t> has been marked as paid.</p>\n"
"                <br>\n"
"                <p>If you have any questions, please contact us at <a t-att-href=\"'mailto:support@merctrans.com'\" style=\"color: #875A7B;\">support@merctrans.com</a></p>\n"
"                <br>\n"
"                <p>Thank you.</p>\n"
"            </div>\n"
"        "
msgstr ""

#. module: morons
#: model:mail.template,body_html:morons.email_template_po_accepted
msgid ""
"<div style=\"margin: 0px; padding: 0px; font-size: 13px;\">\n"
"                <p>The task <t t-out=\"object.name or ''\"></t> has been accepted by <t t-out=\"object.contributor_id.name or ''\"></t></p>\n"
"            </div>\n"
"        "
msgstr ""

#. module: morons
